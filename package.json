{
  "name": "wcag-ui",
  "version": "0.0.14",
  "description": "wcagUI Library",
  "repository": "git@github.com:devdojo-it/wcag-ui.git",
  "author": "DevDojo IT (https://devdojo.it), Exeen (https://exeen.it)",
  "contributors": [
    "Emiliano Pisu <pixu1980@github>",
    "Massimo Artizzu <maxart2501@github>",
    "Mirela Rosca <mirirosca@github>",
    "Marco Pollacci <marcopollacci@github>",
    "Valeria Salis <vallss@github>",
    "Alessio Tortolini <alestormoody@github>",
    "Christian Nastasi <cnastasi@github>",
    "Salvatore Tropea <salvotropea96@github>",
    "Gabriele Falasca <gabrycaos@github>"
  ],
  "license": "MIT",
  "scripts": {
    "precommitlint": "pnpm run lint",
    "commitlint": "commitlint --from-last-tag",
    "clear:cache": "rm -rf .parcel-cache dist",
    "clear": "pnpm run clear:cache && rm -rf node_modules pnpm-lock.yaml",
    "reset": "pnpm run clear && pnpm install && pnpm approve-builds",
    "prestart": "pnpm run clear:cache && pnpm run build:packages",
    "start": "parcel ./src/index.html",
    "build:packages": "node --experimental-modules ./scripts/build.mjs",
    "prebuild": "pnpm run clear:cache",
    "build": "parcel build ./src/index.html --no-source-maps --public-url ./",
    "release:packages": "node --experimental-modules ./scripts/release.mjs",
    "lint": "biome lint .",
    "lint:fix": "biome lint . --write"
  },
  "dependencies": {
    "@wcag-ui/accordion": "workspace:^0.0.14",
    "@wcag-ui/button": "workspace:^0.0.14",
    "@wcag-ui/checkbox": "workspace:^0.0.14",
    "@wcag-ui/core": "workspace:^0.0.14",
    "@wcag-ui/details": "workspace:^0.0.14",
    "@wcag-ui/dialog": "workspace:^0.0.14",
    "@wcag-ui/dom": "workspace:^0.0.14",
    "@wcag-ui/foundations": "workspace:^0.0.14",
    "@wcag-ui/grid-system": "workspace:^0.0.14",
    "@wcag-ui/iconography": "workspace:^0.0.14",
    "@wcag-ui/input": "workspace:^0.0.14",
    "@wcag-ui/radio": "workspace:^0.0.14",
    "@wcag-ui/scroll-spy": "workspace:^0.0.14",
    "@wcag-ui/select": "workspace:^0.0.14",
    "@wcag-ui/switch": "workspace:^0.0.14",
    "@wcag-ui/textarea": "workspace:^0.0.14",
    "@wcag-ui/tooltip": "workspace:^0.0.14",
    "@wcag-ui/typography": "workspace:^0.0.14",
    "highlight.js": "11.11.1"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.1",
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "esbuild": "0.25.5",
    "generate-changelog": "1.8.0",
    "husky": "9.1.7",
    "jsdoc": "4.0.4",
    "parcel": "2.15.4",
    "posthtml": "0.16.6",
    "posthtml-extend": "0.6.5",
    "posthtml-highlight": "3.0.0",
    "posthtml-include": "2.0.1",
    "posthtml-markdownit": "3.1.2",
    "realfavicon": "0.4.18",
    "svgo": "3.3.2"
  }
}
