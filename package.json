{
  "name": "wcag-ui",
  "version": "0.0.0",
  "description": "wcagUI Library",
  "repository": "git@github.com:devdojo-it/wcag-ui.git",
  "author": "DevDojo IT (https://devdojo.it), Exeen (https://exeen.it)",
  "contributors": [
    "Emiliano Pisu <pixu1980@github>",
    "Massimo Artizzu <maxart2501@github>",
    "Mirela Rosca <mirirosca@github>",
    "Marco Pollacci <marcopollacci@github>",
    "Valeria Salis <vallss@github>",
    "Alessio Tortolini <alestormoody@github>",
    "Christian Nastasi <cnastasi@github>",
    "Salvatore Tropea <salvotropea96@github>"
  ],
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "commitlint": "commitlint --from-last-tag",
    "clear:cache": "rm -rf .parcel-cache dist",
    "clear": "yarn clear:cache && rm -rf node_modules yarn.lock package-lock.json",
    "reset": "yarn clear && yarn",
    "prestart": "yarn clear:cache",
    "start": "parcel ./src/index.html",
    "build:packages": "node --experimental-modules ./scripts/build.mjs",
    "prebuild": "yarn clear:cache",
    "build": "parcel build ./src/index.html --no-source-maps --public-url ./",
    "release": "node --experimental-modules ./scripts/release.mjs"
  },
  "workspaces": [
    "packages/**/*"
  ],
  "dependencies": {
    "@wcag-ui/core": "0.0.0",
    "@wcag-ui/dom": "0.0.0",
    "@wcag-ui/foundations": "0.0.0",
    "@wcag-ui/grid-system": "0.0.0",
    "@wcag-ui/iconography": "0.0.0",
    "@wcag-ui/typography": "0.0.0",
    "@wcag-ui/accordion": "0.0.0",
    "@wcag-ui/button": "0.0.0",
    "@wcag-ui/checkbox": "0.0.0",
    "@wcag-ui/details": "0.0.0",
    "@wcag-ui/dialog": "0.0.0",
    "@wcag-ui/input": "0.0.0",
    "@wcag-ui/radio": "0.0.0",
    "@wcag-ui/select": "0.0.0",
    "@wcag-ui/scroll-spy": "0.0.0",
    "@wcag-ui/switch": "0.0.0",
    "@wcag-ui/tooltip": "0.0.0",
    "highlight.js": "11.11.1"
  },
  "devDependencies": {
    "@commitlint/cli": "19.8.1",
    "@commitlint/config-conventional": "19.8.1",
    "generate-changelog": "1.8.0",
    "husky": "9.1.7",
    "jsdoc": "4.0.4",
    "lightningcss": "1.30.1",
    "lightningcss-cli": "1.30.1",
    "parcel": "2.15.2",
    "posthtml": "0.16.6",
    "posthtml-extend": "0.6.5",
    "posthtml-highlight": "3.0.0",
    "posthtml-include": "2.0.1",
    "posthtml-markdownit": "3.1.1",
    "realfavicon": "0.4.18",
    "rolldown": "1.0.0-beta.10",
    "rollup-plugin-swc3": "0.12.1",
    "svgo": "3.3.2",
    "yarn": "1.22.22"
  }
}
